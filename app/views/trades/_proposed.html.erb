<table>
  <thead>
    <tr>
      <th> Team </th>
      <th> Date Requested </th>
      <th> </th>
    </tr>
  </thead>
  <% @proposedtrades.each do |trade| %>
    <tr>
      <td> <%=trade.proposing_team.name %> </td>
      <td> <%=trade.created_at.strftime('%m/%d/%y') %></td>
      <td> <%=link_to 'Review Trade', edit_trade_path %> </td>
      <% end %>
    </tr>
</table>

